if &compatible
  set nocompatible
end

filetype off
set rtp+=~/.vim/bundle/Vundle.vim/
call vundle#begin()

" Let Vundle manage Vundle
Plugin 'gmarik/Vundle.vim'

" plugins
Plugin 'kien/ctrlp.vim'
Plugin 'tomtom/tcomment_vim'

" color schemes
Plugin 'croaky/vim-colors-github'
Plugin 'flazz/vim-colorschemes'

call vundle#end()
filetype on
